<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item android:id="@android:id/background">
    <shape>
        <corners android:radius="5dip" />
        <gradient android:angle="270" android:centerColor="#ffdbcd" android:centerY="0.75"
            android:endColor="#ffdbcd" android:startColor="#ffdbcd" />
    </shape>
</item>
<item android:id="@android:id/progress">
<clip>
    <shape>
        <corners android:radius="5dip" />
        <gradient android:startColor="@color/base_orange" android:centerColor="@color/base_orange" android:centerY="0.75"
            android:endColor="@color/base_orange" android:angle="270" />
    </shape>
</clip>
</item>
<item android:id="@android:id/secondaryProgress">
<clip>
    <shape>
        <corners android:radius="5dip" />
        <gradient android:startColor="#fed7ec" android:centerColor="#fed7ec" android:centerY="0.75"
            android:endColor="#fed7ec" android:angle="270" />
    </shape>
</clip>
</item>
</layer-list>